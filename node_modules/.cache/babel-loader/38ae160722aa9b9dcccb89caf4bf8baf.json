mail } from '../../utils/validation';\r\nimport { checkUserAPI, signupUserAPI } from '../../api/index';\r\n\r\nexport default {\r\n  name: 'VueFrontendJoinform',\r\n\r\n  data() {\r\n    return {\r\n      id: '',\r\n      password: '',\r\n      passwordCheck: '',\r\n      email: '',\r\n      idcheck: false,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    isUsernameValid() {\r\n      return validateEmail(this.email);\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    async check() {\r\n      const api = await checkUserAPI({\r\n        params: {\r\n          id: this.id,\r\n        },\r\n      });\r\n      if (api.data == 200) {\r\n        alert('사용 가능한 아이디 입니다.');\r\n        this.idcheck = true;\r\n      } else {\r\n        alert('이미 사용중인 아이디 입니다.');\r\n        this.idcheck = false;\r\n      }\r\n    },\r\n    async join() {\r\n      if (!this.isUsernameValid) {\r\n        alert('이메일 형식을 맞춰주세요');\r\n      } else {\r\n        if (\r\n          this.password !== '' &&\r\n          this.password === this.passwordCheck &&\r\n          this.idcheck\r\n        ) {\r\n          await signupUserAPI({\r\n            params: {\r\n              id: this.id,\r\n              password: this.password,\r\n              email: this.email,\r\n            },\r\n          });\r\n          alert('회원가입에 성공하였습니다.');\r\n          this.$router.push('/Login');\r\n        } else {\r\n          alert('아이디 또는 비밀번호를 확인해주세요');\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.innerss {\r\n  background-color: #545c64;\r\n  padding: 20px 20px;\r\n  margin: 1% 20%;\r\n  border-radius: 4;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/auth"}]}