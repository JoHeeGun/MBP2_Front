  await insertReplyAPI({\r\n          params: {\r\n            id: this.id,\r\n            content: this.content,\r\n            board_num: this.$route.params.id,\r\n          },\r\n        });\r\n        this.$store.dispatch('community/communityDetail', {\r\n          board_num: this.$route.params.id,\r\n        });\r\n        this.content = '';\r\n      } catch (message) {\r\n        alert(message);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/components/community"}]}