rn {\r\n      options: [\r\n        {\r\n          value: '자유',\r\n          label: '자유게시판',\r\n        },\r\n        {\r\n          value: '질문',\r\n          label: '질문게시판',\r\n        },\r\n      ],\r\n      title: '',\r\n      content: '',\r\n      category: '',\r\n    };\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const { data } = await getBoardDetailAPI({\r\n        params: {\r\n          board_num: this.$route.params.id,\r\n        },\r\n      });\r\n      this.title = data.title;\r\n      this.content = data.content;\r\n    } catch (message) {\r\n      alert(message);\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    goBack() {\r\n      this.$router.go(-1);\r\n    },\r\n    async edit() {\r\n      try {\r\n        await updateBoardAPI({\r\n          params: {\r\n            board_num: this.$route.params.id,\r\n            title: this.title,\r\n            content: this.content,\r\n          },\r\n        });\r\n        this.$router.go(-1);\r\n      } catch (message) {\r\n        alert(message);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>\r\n"],"sourceRoot":"src/components/community"}]}